      "link":"${locServer}",
      "game": "I'm watching on Marquee Sports. \r\n ${appServer}/game/${seoName}",
      "program": "I'm watching on Marquee Sports. \r\n ${appServer}/video/${seoName}",
      "channel": "I'm watching on Marquee Sports. \r\n ${appServer}/channel/${seoName}",
      "news": "I'm watching on Marquee Sports. \r\n ${link}"
    },
    "es.app.purchase":{
      "keepRawJSON": true,
      "isStore": true,
      "skuFormat": "lowercase",
      "descFromStore": true,
      "priceFromStore": true,
      "mobileSupportSKUs": ".*mrqu_premium_monthly.*|.*mrqu_premium_annual.*",
      "subSKUs": ".*annual.*|.*monthly.*",
      "ppvSKUs": ".*SINGLEGAME.*",
      "freeSKUs": ".*FREESKU.*",
      "holderSKUs": {
        "mrqu_premium_annual": "amazon",
        "mrqu_premium_monthly": "monthly",
        "BWEEKLY": "",
        "BAPMONTHLY": "",
        "BNEWWEEKLY": ""
      },
      "consumableSKUs": [
        {
          "rule": ".*BLPPDAY.*",
          "expireDate": "259200"
        }
      ],
      "store": {
        "validSKUs": ".*\\.2023\\..*",
        "skuRule": "com.marqueesports.prod.amazon.${sku}",
        "skuRuleSub": "com.marqueesports.prod.amazon.${skuHolder}",
        "skuRulePPV": "com.marqueesports.prod.amazon.${sku}",
        "base64EncodedPublicKey": "xxxx, only for android"
      },
      "dev": {
        "validSKUs": ".*\\.2023\\..*",
        "skuRule": "com.marqueesports.staging.amazon.${sku}",
        "skuRuleSub": "com.marqueesports.staging.amazon.${skuHolder}",
        "skuRulePPV": "com.marqueesports.staging.amazon.${sku}",
        "shareSecret": "xxxx - only for ios",
        "base64EncodedPublicKey": "xxxx, only for android"
      }
    },
